{
	"name": "cloud-setup",
	"version": "2.0.4",
	"description": "How to setup the fullstack cloud (with deno or github interface)",
	"main": "index.js",
	"scripts": {
		"install:api": "node index.js api && cd cloud-api && yarn",
		"install:github": "node index.js github && cd cloud-github && yarn",
		"install:deno": "node index.js deno && cd cloud-deno && yarn",
		"setup:api": "cd cloud-api && yarn setup",
		"build:api": "cd cloud-api && yarn build",
		"build:github": "cd cloud-github && yarn build",
		"build:deno": "cd cloud-deno && yarn build",
		"start:api": "cd cloud-api && yarn start",
		"start:github": "cd cloud-github && yarn start",
		"start:deno": "cd cloud-deno && yarn start"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iswilljr/cloud-setup.git"
	},
	"keywords": [
		"cloud",
		"home-cloud",
		"api",
		"github",
		"deno"
	],
	"author": "iswilljr",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/iswilljr/cloud-setup/issues"
	},
	"homepage": "https://github.com/iswilljr/cloud-setup#readme",
	"dependencies": {
		"shelljs": "^0.8.5"
	}
}
